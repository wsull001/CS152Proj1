function main;
beginparams
endparams
beginlocals
n : integer;
endlocals
beginbody
  read n;
  if n <> 70 then
    n := -n - 1;
  else
    n := n + 1;
  endif;
  write n;
  read n;
  while n > 0 beginloop
    write n;
    n := n - 1;
    if n == 4 then
      continue;
    endif;
    write n;
  endloop;
  return n;
endbody
