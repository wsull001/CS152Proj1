function test;
beginparams
  i : integer;
  k : integer;
endparams
beginlocals
endlocals
beginbody
  return i - k;
endbody


function main;
beginparams
endparams
beginlocals
n : integer;
j : integer;
endlocals
beginbody
  read n;
  if n <> 70 then
    n := -n - 1;
  else
    n := n + 1;
  endif;
  write n;
  read n;
  while n > 0 beginloop
    write n;
    n := n - 1;
    if n == 4 then
      continue;
    endif;
    write n;
  endloop;
  read n;
  read j;
  do beginloop
    write n;
    n := test( n, j );
  endloop while n > 0;
endbody
